log_prob <- function(x){banana(x, 0.05)}
iter <- 1000
x0   <- c(0,0)
sig  <- 0.1
x    <- mc2:::metropolis(log_prob, x0, iter, sig)
df <- data.frame(x[,1], x[,2], 1:iter)
colnames(df) <- c("x1", "x2", "iter")
plt <- ggplot(df, aes(x = x1, y = x2)) + geom_point(alpha = 0.5)
plt
